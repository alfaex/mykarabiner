{
    "title": "My Personal Modifications",
    "rules": [
        {
            "description": "Change Home to ⌘ + ← and End to ⌘ + →",
            "manipulators": [
                {
                    "from": {
                        "key_code": "home"
                        },
                    "to": [
                        {
                            "key_code": "left_arrow",
                            "modifiers": [
                                "command"
                            ]
                        }
                    ],
                    "type": "basic"
                },
                {
                    "from": {
                        "key_code": "end"
                        },
                    "to": [
                        {
                            "key_code": "right_arrow",
                            "modifiers": [
                                "command"
                            ]
                        }
                    ],
                    "type": "basic"
                }
            ]
        }
    ]
}
                        